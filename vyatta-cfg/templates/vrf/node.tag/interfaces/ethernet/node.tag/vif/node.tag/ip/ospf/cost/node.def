type: u32
help: Interface cost
syntax:expression: $VAR(@) >= 1 && $VAR(@) <= 65535; "Must be between 1-65535"
val_help: u32:1-65535; OSPF interface cost

update:sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../../../../@) \
	-c "configure terminal" \
	-c "interface $VAR(../../../../@).$VAR(../../../@)" \
	-c "ip ospf cost $VAR(@)"
delete:sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../../../../@) \
	-c "configure terminal" \
	-c "interface $VAR(../../../../@).$VAR(../../../@)" \
	-c "no ip ospf cost"
