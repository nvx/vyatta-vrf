type: u32
help: Link state transmit delay
default: 1
syntax:expression: $VAR(@) >= 1 && $VAR(@) <= 65535; "Must be between 1-65535"
val_help: u32:1-65535; Transmit delay in seconds (default 1)

update:sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../../../@) \
	-c "configure terminal" -c "interface $VAR(../../../@)" -c "ip ospf transmit-delay $VAR(@)"
delete:sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../../../@) \
	-c "configure terminal" -c "interface $VAR(../../../@)" -c "no ip ospf transmit-delay"
