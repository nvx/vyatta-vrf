tag:
type: ipv4
help: Neighbor IP address
create:expression: "sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../@) \
       -c \"configure terminal\" \
       -c \"router ospf\" \
       -c \"neighbor $VAR(@)\"; "
delete:expression: "sudo /usr/bin/vtysh -x /var/run/quagga-$VAR(../../../@) \
       -c \"configure terminal\" \
       -c \"router ospf\" \
       -c \"no neighbor $VAR(@)\"; "
